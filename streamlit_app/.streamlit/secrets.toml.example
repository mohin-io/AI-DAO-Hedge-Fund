# Example secrets file for Streamlit Cloud
# Copy this to secrets.toml and fill in your values
# DO NOT commit secrets.toml to git!

# API Configuration
[api]
url = "http://localhost:8000"
ws_url = "ws://localhost:8000/ws"

# Blockchain Configuration
[blockchain]
rpc_url = "https://sepolia.infura.io/v3/YOUR_API_KEY"
chain_id = 11155111

# Smart Contract Addresses
[contracts]
dao_governance = "0x5FbDB2315678afecb367f032d93F642f64180aa3"
treasury_manager = "0xe7f1725E7734CE288F8367e1Bb143E90bb3F0512"
agent_registry = "0x9fE46736679d2D9a65F0992F2272dE9f3c7fa6e0"

