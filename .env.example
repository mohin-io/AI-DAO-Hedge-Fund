# AI DAO Hedge Fund - Environment Variables Template
# Copy this file to .env and fill in your values

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/aidao
REDIS_URL=redis://localhost:6379

# Blockchain - Ethereum Sepolia Testnet
SEPOLIA_RPC_URL=https://rpc.sepolia.org
# Get from https://infura.io or https://alchemy.com
INFURA_API_KEY=your_infura_api_key_here
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Private key for contract deployment (DO NOT COMMIT!)
PRIVATE_KEY=your_private_key_here

# Etherscan API for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# Blockchain Addresses (filled after deployment)
DAO_GOVERNANCE_ADDRESS=
TREASURY_MANAGER_ADDRESS=
AGENT_REGISTRY_ADDRESS=

# Market Data APIs
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
POLYGON_API_KEY=your_polygon_io_key_here

# Trading Configuration
INITIAL_BALANCE=100000
TRANSACTION_COST=0.001
SLIPPAGE=0.0005

# ML/Training
MODEL_SAVE_PATH=./models
TENSORBOARD_LOG_DIR=./logs
TRAINING_TIMESTEPS=500000

# Monitoring
GRAFANA_ADMIN_PASSWORD=admin
PROMETHEUS_RETENTION=15d

# Security
SECRET_KEY=your_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Frontend
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Email Notifications (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Telegram Notifications (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# AWS (for production deployment)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=

# Production Settings
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=INFO
